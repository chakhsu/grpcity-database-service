  "description": "Database Service: a production-ready database microservice based on gRPCity.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --trace-warnings ./bin/server.js",
    "test": "ava",
